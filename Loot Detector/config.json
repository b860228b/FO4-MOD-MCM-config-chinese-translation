    "minMcmVersion": 1,
    "content":[{
            "text": "<b>LOOT DETECTOR</b>",
            "type": "section", "html": true
        },{
            "type": "spacer"
        },{
            "text": "This mod adds contextual highlighting to many world objects when you take the <b><i>Scrapper</i></b> perk. Here you can customize all of the mod's features to match your playstyle.",
            "type": "text", "html": true
        },{
            "type": "section", "text": ""
        },{
            "type": "switcher", "text": "Corpse Vision",
            "help": "Highlight corpses and ashpiles with loot.",
            "valueOptions": { "sourceType": "GlobalValue", "sourceForm": "Loot Detector.esp|80e" },
            "action": { "type": "CallFunction", "form": "Loot Detector.esp|f9e", "function": "MCMApply" }
        },{
            "type": "switcher", "text": "KO Vision",
            "help": "Highlight unconscious (e.g drugged, knocked out, or powered down) actors.",
            "valueOptions": { "sourceType": "GlobalValue", "sourceForm": "Loot Detector.esp|815" },
            "action": { "type": "CallFunction", "form": "Loot Detector.esp|f9e", "function": "MCMApply" }
        },{
            "type": "switcher", "text": "Sandman Vision",
            "help": "Highlight sleeping friends and foes.",
            "valueOptions": { "sourceType": "GlobalValue", "sourceForm": "Loot Detector.esp|802" },
            "action": { "type": "CallFunction", "form": "Loot Detector.esp|f9e", "function": "MCMApply" }
        },{
            "type": "switcher", "text": "Loot Vision",
